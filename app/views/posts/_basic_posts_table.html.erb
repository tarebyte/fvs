<%# <%= will_paginate @posts, renderer: BootstrapPagination::Rails %>

<table class="table table-striped table-bordered">
  <tbody>
  <% @posts.each do |post| %>
    <tr id="<%= post.id %>">
      <td><%= link_to post.title, user_post_path(post.author, post) %> by: <%= link_to post.author.name, user_path(post.author) %></td>
  </tr>
<% end %>
  </tbody>
</table>
